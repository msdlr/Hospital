<Window x:Class="prototipo_hospital.NumeroPaciente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prototipo_hospital"
        mc:Ignorable="d"
        Title="Número de Paciente" Height="200" Width="300" ResizeMode="NoResize">
    <Grid>
        <Button Name="botonabrirpaciente" Content="Abrir" HorizontalAlignment="Left" Margin="110,111,0,0" VerticalAlignment="Top" Width="75" Click="botonabrirpaciente_Click" KeyDown="botonabrirpaciente_Click" IsDefault="True" />
        <TextBox Name="campoIDPaciente" HorizontalAlignment="Left" Height="24" Margin="11,66,0,0" VerticalAlignment="Top" Width="272" />
        <Label  Name="abrirPaciente" Content="Introduzca el numero del paciente:" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Width="216"/>

    </Grid>
</Window>
